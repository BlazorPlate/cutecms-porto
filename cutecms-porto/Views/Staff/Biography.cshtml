@model cutecms_porto.Areas.Identity.Models.DBModel.Employee
@using System.Threading
<form role="form" action="#" method="post">
    <div class="form-group">
        @Html.ActionLink(Resources.Resources.ViewCV, "Download", new { FilePath = Model.ResumeFilePath, FileName = Model.ResumeFileName })
    </div>
    <div class="form-group">
        @Html.Label(Resources.Resources.Biography)
        @Html.Raw(Model.Biography)
    </div>
</form>